<?xml version="1.0" encoding="UTF-8"?>
<class:Model
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:class="class"
    xsi:schemaLocation="class /Class2Relational/Class.ecore">
  <elem xsi:type="class:DataType"
      name="String"/>
  <elem xsi:type="class:Class"
      name="Book">
    <attr name="Title"
        type="//@elem.0"/>
    <attr name="Editor"
        type="//@elem.0"/>
    <attr name="Authors"
        multiValued="true"
        type="//@elem.0"/>
  </elem>
</class:Model>
